.lob {
  font-family: Lobster, GENERIC_NAME;
}

.if {
  font-family: 'Indie Flower', GENERIC_NAME;
}

.standOut {
  font-family: monospace;
  font-size: 20px;
  background-color: grey;
  color: aliceblue;
  font-style: italic;
}

.whopper{
  image-orientation: flip;
  border-color:cornflowerblue;
  border-width: 5px;
  border-style: dotted;
}

.roj {
  border-color: cyan;
  border-width: 15px;
  border-style: groove;
  
}

.duck {
  border-color:blueviolet;
  border-width: 20px;
  border-style: double;
  border-radius: 5px; 
}
